#include <iostream> // =stdio.h, 입출력에 관련된 라이브러리가 모여 있음

using namespace std; // name space

int main() {

	cout << "Hello World!!\n\n"; //화면에 문자열 출력
	cout << "Kim ChulSoo\n";
	cout << "C++ is easy" << endl; // \n은 " " 안에 있어야 하지만 endl은 하나의 명령어
	printf("Hello C++\n\n"); // printf가 속도가 훨씬 빠름

	// 구구단 출력
	cout << "3 x 1 = 3\n";
	cout << "3 x 2 = 6" << endl;
	cout << "3 x 3 = 9" << endl;

	return 0;
}

